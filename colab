        "import numpy as np\n",
        "from numpy.linalg import det, inv\n",
        "\n",
        "A19a = np.array([\n",
        "    [3, 2, 5, 6],\n",
        "    [6, 5, 6, 7],\n",
        "    [0, 0, 4, 8],\n",
        "    [0, 0, 2, 5]\n",
        "])\n",
        "\n",
        "A19b = np.array([\n",
        "    [0, 5, 2, 3, -1],\n",
        "    [-5, 0, 6, 7, 2],\n",
        "    [-2, -6, 0, 4, 8],\n",
        "    [-3, -7, -4, 0, 1],\n",
        "    [1, -2, -8, -1, 0]\n",
